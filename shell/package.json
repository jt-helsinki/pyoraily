  "scripts": {
    "clean": "rm -rf node_modules/.cache/**; rm -rf build; rm -rf coverage",
    "clear-jest": "jest --clearCache",
    "start": "DISABLE_ESLINT_PLUGIN=true NODE_ENV=development ESLINT_NO_DEV_ERRORS=true node scripts/start.js",
    "build": "DISABLE_ESLINT_PLUGIN=true NODE_ENV=production node scripts/build.js --display-error-details --trace-deprecation ",
    "format": "prettier --write --ignore-unknown \"**/src/**/*.*\"",
    "format:check": "prettier --check --ignore-unknown \"**/src/**/*.*\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "sls:deploy": "npm run clean; npm run build; sls deploy --config serverless.yaml --stage production --verbose --conceal",
    "sls:deploy2": "sls deploy --config serverless.yaml --stage production --verbose --conceal",
    "sls:undeploy": "sls remove --config serverless.yaml --stage production --verbose",
    "test": "NODE_ENV=test node scripts/test.js",
    "test:debug": "NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand --detectOpenHandles",
    "patch": "cp -r ./patches/* ./node_modules"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
